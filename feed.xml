        &lt;label for=&quot;grid-size-y&quot;&gt;Grid Size Y&lt;/label&gt;
        &lt;input id=&quot;grid-size-y&quot; class=&quot;form-control&quot; type=&quot;numeric&quot; value=&quot;&quot;&gt;
      &lt;/div&gt;
      --&gt;
      &lt;div class=&quot;form-group&quot;&gt;
